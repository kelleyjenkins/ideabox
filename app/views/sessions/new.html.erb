
<div class="panel-body">
  <h1> Sign In to Your Idea Box! </h1>
  <div class="row">
    <div class="col-lg-12">
      <%= bootstrap_form_tag url: login_path do |f| %>
        <%= f.email_field :email %>
        <%= f.password_field :password %>
        <%= f.submit "Sign In"  %>
        <% end %></br>
        <%= link_to "Don't Have Account? Register Here!", new_user_path %>
      </div>
    </div>
  </div>
